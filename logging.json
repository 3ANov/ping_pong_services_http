{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "default_formatter": {
      "format": "%(asctime)s - %(name)s - %(levelname)s: %(message)s"
    }
  },
  "handlers": {
    "stream_handler": {
      "class": "logging.StreamHandler",
      "formatter": "default_formatter"
    }
  },
  "loggers": {
    "root": {
      "handlers": [
        "stream_handler"
      ],
      "level": "DEBUG",
      "propagate": true
    }
  }
}